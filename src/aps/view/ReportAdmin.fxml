<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="556.0" prefWidth="666.0" styleClass="body" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aps.view.ReportAdminController">
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="19.0" prefWidth="666.0" style="-fx-background-color: #84C7D2;" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="btnBack" buttonType="RAISED" lineSpacing="10.0" onAction="#goBack" ripplerFill="#175668" style="-fx-background-color: #84C7D2;" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </JFXButton>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <Label text="Generate report">
               <font>
                  <Font name="Century Gothic Bold" size="18.0" />
               </font>
            </Label>
         </children>
         <effect>
            <InnerShadow />
         </effect>
      </HBox>
   </top>
   <bottom>
      <HBox prefHeight="11.0" prefWidth="666.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <JFXTabPane fx:id="reportTab" styleClass="jfx-tab-pane" stylesheets="@styles.css" BorderPane.alignment="CENTER" />
   </center>
   <effect>
      <InnerShadow />
   </effect>
</BorderPane>
